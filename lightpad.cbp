<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="lightpad" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/lightpad" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D__WXDEBUG__" />
					<Add directory="$(#wx)/lib/gcc_dll/mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/mswu" />
				</ResourceCompiler>
				<Linker>
					<Add library="libwxmsw31u.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/lightpad" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="$(#wx)/lib/gcc_dll/mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/mswu" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="libwxmsw30u.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-Wall" />
			<Add directory="$(#wx)/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#wx)/include" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
		</Linker>
		<Unit filename="FilePanel.cpp" />
		<Unit filename="FilePanel.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
